@mixin page_tab($color: none){
  max-width:$page_width - 4.5em;
  min-height:20em;
  border:$border;
  @include opacity(60);
    
  @if $color != none {
    background: lighten($color, 0%);
    padding:2em;
   @media screen and (max-width: $small_width){
       display:none;
    }
  }
}

@each $tab_color in green, cream, pink, palepink, purple, blue {
  $colourr:none;
  ##{$tab_color} {
    @if($tab_color == green){
    $colourr: $green;
    }
    @if($tab_color == cream){
    $colourr: $cream;
    }
    @if($tab_color == pink)
    {
    $colourr: $pink;
    }
     @if($tab_color == palepink)
    {
    $colourr: $palepink;
    }
    @if($tab_color == blue)
    {
    $colourr: $babyblue;
    }
    @if($tab_color == purple)
    {
    $colourr: $purple;
    }

  @include page_tab($colourr); 
  } 
}



